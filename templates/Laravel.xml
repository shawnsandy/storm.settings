<templateSet group="Laravel">
  <template name="_phpunit" value=" /**&#10;     * $DESCRIPTION$&#10;     * @test&#10;     */&#10;    function $can_view_static_pages$(){&#10;      &#10;    }" description="Create a test object" toReformat="false" toShortenFQNames="true">
    <variable name="DESCRIPTION" expression="capitalize(String)" defaultValue="Describe test" alwaysStopAt="true" />
    <variable name="can_view_static_pages" expression="decapitalize(String)" defaultValue="test_name_here" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="_addroute" value="Route::get(&quot;/$NAME$&quot;, &quot;$CONTORLLER$@$METHOD$&quot;);" description="add a route" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CONTORLLER" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="_form" value="&lt;p&gt;&lt;label for=&quot;$NAME$&quot;&gt;$LABEL$&lt;/label&gt;&#10;    {!! Form::text('$NAME$', null, ['class' =&gt; 'form-control']) !!}&#10;&lt;/p&gt;" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="name" alwaysStopAt="true" />
    <variable name="LABEL" expression="" defaultValue="label" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="Twig" value="true" />
      <option name="PHP" value="true" />
      <option name="PHP Comment" value="true" />
      <option name="PHP String Literal" value="true" />
    </context>
  </template>
  <template name="quick-table" value=" &lt;table class=&quot;table&quot;&gt;&#10;                    &lt;thead&gt;&#10;                        &lt;tr&gt;&#10;                            &lt;td&gt;$Labelhere$&lt;/td&gt;&#10;                        &lt;/tr&gt;&#10;&#10;                    &lt;/thead&gt;&#10;                    &lt;tbody&gt;&#10;                        &lt;tr&gt;&#10;                            &lt;td&gt;Descrtiption here&lt;/td&gt;&#10;                            &lt;td&gt;&#10;                               &lt;button&gt;View&lt;/button&gt;&#10;                            &lt;/td&gt;&#10;                        &lt;/tr&gt;&#10;                   &#10;                    &lt;/tbody&gt;&#10;&#10;                &lt;/table&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="Labelhere" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="true" />
      <option name="XML" value="true" />
      <option name="XML_TEXT" value="true" />
      <option name="JSON" value="true" />
      <option name="SQL" value="true" />
      <option name="JSP" value="true" />
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
      <option name="Twig" value="true" />
      <option name="CUCUMBER_FEATURE_FILE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
      <option name="CoffeeScript" value="true" />
      <option name="PHP" value="true" />
      <option name="PHP Comment" value="true" />
      <option name="PHP String Literal" value="true" />
      <option name="HAML" value="true" />
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>